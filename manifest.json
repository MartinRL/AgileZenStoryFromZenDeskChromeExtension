{
   "name": "Create AgileZen Story from ZenDesk Ticket",
   "version": "2.0.0",
   "manifest_version": 2,
   "description": "Creates an AgileZen story from the ZenDesk ticket in question.",
   "options_page": "options.html",
   "browser_action": {
     "default_icon": "icon.png"
   },
   "permissions": [
       "*://*.zendesk.com/tickets/*",
       "*://helpdesk.mvno.dk/tickets/*",
       "*://*.agilezen.com/api/*",
       "storage"
   ],
   "content_scripts": [{
       "matches": ["*://*.zendesk.com/tickets/*"],
       "matches": ["*://helpdesk.mvno.dk/tickets/*"],
       "css": ["style.css"],
       "js": ["jquery-2.0.3.min.js", "script.js"]
   }]
}
